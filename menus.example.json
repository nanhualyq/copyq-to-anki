  {
    "name": "Youdao",
    "plugin": "youdao",
    "anki": {
      "action": "addNote",
      "params": {
        "note": {
          "deckName": "AI English",
          "modelName": "AH YouDao",
          "fields": {
            "Word": "{{pluginResult.Word}}",
            "Exp": "{{pluginResult.Exp}}",
            "Phone": "{{pluginResult.Phone}}",
            "Url": "{{copyq.getUrl()}}",
            "Title": "{{copyq.getTitle()}}"
          },
          "audio": [
            {
              "url": "https://translate.google.com/translate_tts?ie=UTF-8&client=tw-ob&tl=en&q={{pluginResult.Word}}",
              "filename": "google-{{Date.now()}}.mp3",
              "fields": ["Audio"]
            }
          ]
        }
      }
    }
  },
  {
    "name": "Table Row",
    "plugin": "tr",
    "anki": {
      "action": "addNote",
      "params": {
        "note": {
          "deckName": "Default",
          "modelName": "Basic",
          "fields": {
            "Front": "{{pluginResult.eq(1).html() || ''}}",
            "Back": "{{pluginResult.eq(0).html() || ''}}",
            "Url": "{{copyq.getUrl()}}",
            "Title": "{{copyq.getTitle()}}"
          }
        }
      }
    }
